{
  "name": "vue-application",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve --host 127.0.0.1 --port 5000",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "apollo": "vue-cli-service apollo:dev --generate-schema",
    "apollo:schema:generate": "vue-cli-service apollo:schema:generate",
    "apollo:schema:publish": "vue-cli-service apollo:schema:publish",
    "apollo:start": "vue-cli-service apollo:start"
  },
  "dependencies": {
    "core-js": "^3.6.4",
    "graphql-type-json": "^0.2.1",
    "laravel-elixir-vue-2": "^0.3.0",
    "module": "^1.2.5",
    "node-sass": "^7.0.0",
    "npm": "^6.13.7",
    "sass-loader": "^8.0.2",
    "vue": "^2.6.11",
    "vue-apollo": "^3.0.0-beta.11",
    "vue-loader": "^15.9.0",
    "vue-router": "^3.1.5",
    "webpack": "^4.41.6"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.2.0",
    "@vue/cli-plugin-eslint": "~4.2.0",
    "@vue/cli-service": "~4.2.0",
    "babel-eslint": "^10.0.3",
    "eslint": "^6.7.2",
    "eslint-plugin-graphql": "^2.1.1",
    "eslint-plugin-vue": "^6.1.2",
    "graphql-tag": "^2.9.0",
    "vue-cli-plugin-apollo": "~0.21.3",
    "vue-template-compiler": "^2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "plugins": [
      "graphql"
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
